{

  "manifest_version": 2,
  "name": "ToMarkdown",
  "version": "1.0",

  "description": "Try to generate a markdown version of current webpage.",

  "icons": {
    "48": "icons/icon.png"
  },

 "browser_action": {
    "default_icon": "icons/icon.png",
    "default_title": "Convert to Markdown",
    "default_popup": "popup/popup.html"
  },

  "permissions": [
    "activeTab",
    "<all_urls>"
  ]

}
